<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.ido.umi.Limit">


    <application
        android:name="app.ido.umi.Limit.RealmTodoApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="app.ido.umi.Limit.ShoppingActivity" />
        <activity android:name="app.ido.umi.Limit.ExplanationActivity" />
        <activity android:name="app.ido.umi.Limit.SaveActivity" />
        <activity android:name="app.ido.umi.Limit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>